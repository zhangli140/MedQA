<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>API</servlet-name>
    <servlet-class>org.zjuwangg.servlet.API</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Numeric</servlet-name>
    <servlet-class>org.zjuwangg.servlet.Numeric</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Relation</servlet-name>
    <servlet-class>org.zjuwangg.servlet.Relation</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoadTherpy</servlet-name>
    <servlet-class>org.zjuwangg.servlet.LoadTherpy</servlet-class>
  </servlet>
    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>org.zjuwangg.server.Servlet</servlet-class>
    </servlet>


    <servlet-mapping>
    <servlet-name>API</servlet-name>
    <url-pattern>/API</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Numeric</servlet-name>
    <url-pattern>/Numeric</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Relation</servlet-name>
    <url-pattern>/Relation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoadTherpy</servlet-name>
    <url-pattern>/LoadTherpy</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
